{
  "manifest_version": 2,

  "name": "Test extension",
  "description": "This extension is a test",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["autofill.js"]
    }
  ]
}